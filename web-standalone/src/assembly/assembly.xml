<assembly>
  <id>winstone</id>
  <formats>
    <format>jar</format>
  </formats>
  <includeBaseDirectory>false</includeBaseDirectory>
  <fileSets>
    <fileSet>
      <directory>src/assembly</directory>
      <outputDirectory></outputDirectory>
      <includes>
        <include>embedded.properties</include>
      </includes>
    </fileSet>
  </fileSets>
  <files>
    <file>
      <source>${basedir}/../LICENSE.txt</source>
      <outputDirectory></outputDirectory>
      <destName>galaxy-LICENSE.txt</destName>
    </file>
    <file>
      <source>${basedir}/../NOTICE.txt</source>
      <outputDirectory></outputDirectory>
      <destName>galaxy-NOTICE.txt</destName>
    </file>
  </files>
  <dependencySets>
    <dependencySet>
      <outputDirectory></outputDirectory>
      <unpack>true</unpack>
      <outputFileNameMapping></outputFileNameMapping>
      <includes>
        <include>org.mule.galaxy:galaxy-web-standalone</include>
        <include>log4j:log4j</include>
        <include>commons-logging:commons-logging</include>
      </includes>
    </dependencySet>
    <dependencySet>
      <unpack>true</unpack>
      <outputDirectory></outputDirectory>
      <outputFileNameMapping></outputFileNameMapping>
      <scope>system</scope>
      <includes>
        <include>org.mortbay.jetty:jetty-runner</include>
      </includes>
    </dependencySet>
    <dependencySet>
      <outputDirectory></outputDirectory>
      <outputFileNameMapping>embedded.war</outputFileNameMapping>
      <includes>
        <include>org.mule.galaxy:galaxy-web</include>
      </includes>
    </dependencySet>
  </dependencySets>
</assembly>
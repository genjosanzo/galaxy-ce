<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:g="http://galaxy.mule.org/cxf/"
       xsi:schemaLocation="
http://galaxy.mule.org/cxf/ http://galaxy.mule.org/schemas/cxf-galaxy.xsd
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <g:galaxy username="admin" password="admin">
    <g:policyQuery>http://localhost:9002/api/repository?q=select artifact where documentType = {http://www.w3.org/2006/07/ws-policy}Policy</g:policyQuery>
  </g:galaxy>
  
</beans>